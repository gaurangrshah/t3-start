{
  "name": "t3-start",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "postbuild": "next-sitemap",
    "env:test": "env-cmd -f .env.test.local",
    "env:dev": "env-cmd -f .env.development.local",
    "studio": "yarn env:dev npx prisma studio",
    "db:migrate": "yarn env:dev npx prisma migrate dev --name $NAME --create-only && yarn prisma generate",
    "db:push": "yarn env:dev npx prisma db push",
    "db:reset": "yarn env:dev npx prisma migrate reset",
    "db:reset-force": "yarn env:dev npx prisma migrate reset --force",
    "db:reset-force:dev": "yarn env:dev yarn db:reset-force && next dev",
    "db:seed": "yarn env:dev npx prisma db seed",
    "gen:theme-typings": "chakra-cli tokens ./chakra.ui/theme/index.ts",
    "test": "jest --watch",
    "ci": "jest --ci",
    "test:dev": "yarn env:test next dev",
    "test:dev:watch": "yarn env:test jest --watch",
    "tdd": "concurrently -k \"yarn env:test next dev\" \"yarn env:test jest --watch\"",
    "test:ci": "yarn env:test jest --ci",
    "test:studio": "yarn env:test npx prisma studio",
    "test:db:migrate": "yarn env:test npx prisma migrate dev --name $NAME --create-only && yarn prisma generate",
    "test:db:push": "yarn env:test npx prisma db push",
    "test:db:reset": "yarn env:test npx prisma migrate reset",
    "test:db:reset-force": "yarn env:test npx prisma migrate reset --force",
    "test:db:reset-force:dev": "yarn env:test yarn db:reset-force && next dev",
    "test:db:seed": "yarn env:test npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.4.3",
    "@chakra-ui/skip-nav": "^2.0.12",
    "@chakra-ui/theme-tools": "^2.0.14",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.20.4",
    "@trpc/client": "^10.5.0",
    "@trpc/next": "^10.5.0",
    "@trpc/react-query": "^10.5.0",
    "@trpc/server": "^10.5.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cross-fetch": "^3.1.5",
    "env-cmd": "^10.1.0",
    "framer-motion": "^7.10.0",
    "jsonwebtoken": "^9.0.0",
    "next": "13.0.7",
    "next-auth": "^4.18.6",
    "next-seo": "^5.15.0",
    "next-sitemap": "3.1.42",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@emotion/jest": "^11.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-next": "13.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "msw": "^0.49.2",
    "prettier": "2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prisma": "^4.7.1",
    "trpc-playground": "^1.0.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "ct3aMetadata": {
    "initVersion": "6.11.3"
  }
}
