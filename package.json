{
  "name": "t3-start",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "postbuild": "next-sitemap",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate dev --name $NAME --create-only && yarn prisma generate",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma migrate reset",
    "db:reset-force": "npx prisma migrate reset --force",
    "pretest:db:reset-force": "env-cmd -f ./.env.test.local npx prisma migrate reset --force",
    "seed": "npx prisma db seed",
    "gen:theme-typings": "chakra-cli tokens ./chakra.ui/theme/index.ts",
    "dev:db-reset": "yarn db:reset-force && env-cmd -f ./.env.test.local next dev",
    "start:db-reset": "yarn pretest:db:reset-force && env-cmd -f ./.env.test.local next build && env-cmd -f ./.env.test.local next start",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:run": "start-server-and-test start:db-reset http://localhost:3000 cy:run",
    "test:e2e:open": "start-server-and-test start:db-reset http://localhost:3000 cy:open",
    "test:e2e:dev": "start-server-and-test dev:db-reset http://localhost:3000 cy:open",
    "pretest:e2e:run": "yarn build"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.4.3",
    "@chakra-ui/skip-nav": "^2.0.12",
    "@chakra-ui/theme-tools": "^2.0.14",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.20.4",
    "@trpc/client": "^10.5.0",
    "@trpc/next": "^10.5.0",
    "@trpc/react-query": "^10.5.0",
    "@trpc/server": "^10.5.0",
    "crypto": "^1.0.1",
    "env-cmd": "^10.1.0",
    "framer-motion": "^7.10.0",
    "next": "13.0.7",
    "next-auth": "^4.18.6",
    "next-seo": "^5.15.0",
    "next-sitemap": "3.1.42",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "cypress": "^12.1.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "13.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prisma": "^4.7.1",
    "start-server-and-test": "^1.15.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "ct3aMetadata": {
    "initVersion": "6.11.3"
  }
}
